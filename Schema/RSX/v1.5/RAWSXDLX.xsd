      <xs:sequence>
        <xs:element ref="dl22.bpost.tussenhoofdnr"/>
        <xs:element ref="romptekst"/>
        <xs:element ref="dl22.tussenhfd.tekst" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute ref="tabposities"/>
      <xs:attribute ref="paginaeinde" />
    </xs:complexType>
  </xs:element>
  <xs:element name ="dl22.bpost.tussenhoofd.nvi">
