      <xs:documentation>Attribuut waarmee wordt aangegeven of het betreffende element mag worden verwijderd</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="ja"/>
        <xs:enumeration value="nee"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="uitbreiden">
    <xs:annotation>
      <xs:documentation>Attribuut waarmee wordt aangegeven of een deel 1, deel 2.1 of verklaring voor deel 2.2 mag worden uitgebreid</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
