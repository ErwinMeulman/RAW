        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="dlx.paragraafvoorloopnr">
    <xs:annotation>
      <xs:documentation>Het nummer voor het paragraafnummer zoals in deel 0 en in deel 1</xs:documentation>
    </xs:annotation>
