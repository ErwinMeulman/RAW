  <xs:annotation>
    <xs:documentation>Deel 2.2 definitie</xs:documentation>
  </xs:annotation>
  <xs:annotation>
    <xs:documentation>Postnummer definities</xs:documentation>
  </xs:annotation>

  <xs:element name="dl22.bpost.tussenhoofdnr">
    <xs:annotation>
      <xs:documentation>1 tot 4 cijferig tussenhoofdnummer</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:pattern value="[0-8]\d{0,3}"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="dl22.bpost.nr6.0">
    <xs:annotation>
      <xs:documentation>6 cijferig bestekspostnummer eindigend op een nul</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:pattern value="[0-8]\d{4}[0]"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="dl22.bpost.nr6.volg">
    <xs:annotation>
      <xs:documentation>6 cijferig bestekspostnummer niet eindigend op een nul</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:pattern value="[0-8]\d{4}[1-9]"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

  <xs:annotation>
    <xs:documentation>Tussenhoofd definities</xs:documentation>
  </xs:annotation>
  <xs:element name="dl22.tussenhfd.tekst" type="tekstblok.eenvoudig">
